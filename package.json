    "@vue/eslint-config-typescript": "^11.0.2",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.5.1",
    "execa": "^5.1.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
